        <activity
            android:name=".java_codes.DatePicker_TimePicker"
            android:label="Date Picker" />
        <activity
            android:name=".java_codes.Spinner"
            android:exported="false"
            android:label="Spinner" />
        <activity android:name=".java_codes.Fragments" />
        <activity
            android:name=".java_codes.Checkbox"
            android:label="CheckBox" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>